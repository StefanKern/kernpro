    "extract-i18n": "nx extract-i18n kernpro",
    "i18n:merge": "xliffmerge --profile xliffmerge.json",
    "i18n:extract-and-merge": "nx extract-i18n kernpro && npm run i18n:merge",
    "build-and-publish-wordcloud": "nx build wordcloud && npm publish ./dist/libs/wordcloud --access public",
    "build-and-publish-kernpro": "nx build kernpro --localize && firebase deploy"
  },

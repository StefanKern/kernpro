{
  "name": "kernpro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-en": "ng serve --configuration=en",
    "build": "ng build --localize",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:kernpro:de": "node dist/kernpro/server/de/server.mjs",
    "serve:ssr:kernpro:en": "node dist/kernpro/server/en/server.mjs",
    "deploy": "firebase deploy",
    "extract-i18n": "ng extract-i18n",
    "i18n:merge": "xliffmerge --profile xliffmerge.json",
    "i18n:extract-and-merge": "ng extract-i18n && npm run i18n:merge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.0",
    "@ks89/angular-modal-gallery": "^13.0.0",
    "animate.css": "^4.1.1",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "luxon": "^3.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/localize": "^20.0.0",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.17",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.18.0",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.8.3"
  }
}
{
  "name": "kernpro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve kernpro",
    "start-en": "nx serve kernpro --configuration=en",
    "build": "nx build kernpro --localize",
    "watch": "nx build kernpro --watch --configuration development",
    "test": "nx test",
    "serve:ssr:kernpro:de": "node dist/kernpro/server/de/server.mjs",
